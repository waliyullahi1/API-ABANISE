{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
       " methods":["GET", "POST", "PUT", "DELTE", "PATCH", "OPIONS"],
       "headers ":{
        "Access-Control-Allow-Origin":"*"
       }
      }
    ]
  }
  